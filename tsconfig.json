{
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es2016"
        ],
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types",
            "typings"
        ]
    }
}